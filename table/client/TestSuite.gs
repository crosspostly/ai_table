rverApiUrl);\n  addTestResult(results, 'üìù addSystemLog', testAddSystemLog);\n}\n\n/**\n * –§–ê–ó–ê 2: –ö–ª–∏–µ–Ω—Ç—Å–∫–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏\n */\nfunction runClientTests(results) {\n  addTestResult(results, 'üß† GM function', testGMFunction);\n  addTestResult(results, 'üìã Menu creation', testMenuCreation);\n  addTestResult(results, 'üîß setupAllCredentialsUI', testSetupCredentialsExists);\n  addTestResult(results, 'üí¨ Chat mode functions', testChatModeExists);\n  addTestResult(results, '‚ö° Smart prompts', testSmartPromptsExists);\n}\n\n/**\n * –§–ê–ó–ê 3: –°–µ—Ä–≤–µ—Ä–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ (–≤—ã–∑–æ–≤ —á–µ—Ä–µ–∑ ThinClient)\n */\nfunction runServerTests(results) {\n  addTestResult(results, 'üåê Server ping', testServerPing);\n  addTestResult(results, 'üîê License validation', testServerLicenseValidation);\n  addTestResult(results, 'üì± VK import API', testVkImportAPI);\n  addTestResult(results, 'üîç OCR service', testOcrServiceAPI);\n}\n\n/**\n * –§–ê–ó–ê 4: –í–µ–±-–∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å\n */\nfunction runWebInterfaceTests(results) {\n  addTestResult(results, 'üåê openWebInterface', testOpenWebInterface);\n  addTestResult(results, 'üìä getSystemStatusData', testGetSystemStatusData);\n  addTestResult(results, 'üì± importSocialPosts', testImportSocialPosts);\n  addTestResult(results, 'üß™ testGeminiConnection', testGeminiConnectionFunction);\n}\n\n/**\n * –§–ê–ó–ê 5: –ò–Ω—Ç–µ–≥—Ä–∞—Ü–∏–æ–Ω–Ω—ã–µ —Ç–µ—Å—Ç—ã\n */\nfunction runIntegrationTests(results) {\n  addTestResult(results, 'üîó Client-Server communication', testClientServerComm);\n  addTestResult(results, 'üìã Sheet operations', testSheetOperations);\n  addTestResult(results, '‚öôÔ∏è Properties management', testPropertiesManagement);\n  addTestResult(results, 'üîÑ Error handling', testErrorHandling);\n}\n\n/**\n * –î–æ–±–∞–≤–∏—Ç—å —Ä–µ–∑—É–ª—å—Ç–∞—Ç —Ç–µ—Å—Ç–∞\n */\nfunction addTestResult(results, testName, testFunction) {\n  results.total++;\n  \n  try {\n    var result = testFunction();\n    \n    if (result && result.success !== false) {\n      results.passed++;\n      results.details.push('‚úÖ ' + testName + ': PASSED');\n      addSystemLog('Test passed: ' + testName, 'INFO', 'TESTING');\n    } else {\n      results.failed++;\n      var errorMsg = result && result.error ? result.error : 'Unknown error';\n      results.errors.push(testName + ': ' + errorMsg);\n      results.details.push('‚ùå ' + testName + ': FAILED - ' + errorMsg);\n      addSystemLog('Test failed: ' + testName + ' - ' + errorMsg, 'WARN', 'TESTING');\n    }\n    \n  } catch (error) {\n    results.failed++;\n    results.errors.push(testName + ': ' + error.message);\n    results.details.push('‚ùå ' + testName + ': ERROR - ' + error.message);\n    addSystemLog('Test error: ' + testName + ' - ' + error.message, 'ERROR', 'TESTING');\n  }\n}\n\n/**\n * –ü–æ–∫–∞–∑–∞—Ç—å —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏—è\n */\nfunction showTestResults(results) {\n  var ui = SpreadsheetApp.getUi();\n  \n  var summary = 'üß™ –†–ï–ó–£–õ–¨–¢–ê–¢–´ –¢–ï–°–¢–ò–†–û–í–ê–ù–ò–Ø\\n\\n';\n  summary += 'üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞:\\n';\n  summary += '‚Ä¢ –í—Å–µ–≥–æ —Ç–µ—Å—Ç–æ–≤: ' + results.total + '\\n';\n  summary += '‚Ä¢ ‚úÖ –ü—Ä–æ—à–ª–∏: ' + results.passed + '\\n';\n  summary += '‚Ä¢ ‚ùå –ù–µ –ø—Ä–æ—à–ª–∏: ' + results.failed + '\\n';\n  summary += '‚Ä¢ üìà –ü—Ä–æ—Ü–µ–Ω—Ç —É—Å–ø–µ—Ö–∞: ' + Math.round((results.passed / results.total) * 100) + '%\\n\\n';\n  \n  if (results.failed > 0) {\n    summary += '‚ùå –ö–†–ò–¢–ò–ß–ï–°–ö–ò–ï –û–®–ò–ë–ö–ò:\\n';\n    for (var i = 0; i < Math.min(results.errors.length, 5); i++) {\n      summary += '‚Ä¢ ' + results.errors[i] + '\\n';\n    }\n    if (results.errors.length > 5) {\n      summary += '‚Ä¢ ... –∏ –µ—â—ë ' + (results.errors.length - 5) + ' –æ—à–∏–±–æ–∫\\n';\n    }\n    summary += '\\nüìã –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –ª–æ–≥–∏ —Å–∏—Å—Ç–µ–º—ã –¥–ª—è –¥–µ—Ç–∞–ª–∏–∑–∞—Ü–∏–∏.';\n  } else {\n    summary += 'üéâ –í–°–ï –¢–ï–°–¢–´ –ü–†–û–®–õ–ò –£–°–ü–ï–®–ù–û!\\n\\n';\n    summary += '‚úÖ –°–∏—Å—Ç–µ–º–∞ –≥–æ—Ç–æ–≤–∞ –∫ —Ä–∞–±–æ—Ç–µ —Å —Ä–µ–∞–ª—å–Ω—ã–º–∏ –¥–∞–Ω–Ω—ã–º–∏.';\n  }\n  \n  ui.alert('–†–µ–∑—É–ª—å—Ç–∞—Ç—ã —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏—è', summary, ui.ButtonSet.OK);\n}\n\n// ===== –¢–ï–°–¢–û–í–´–ï –§–£–ù–ö–¶–ò–ò =====\n\n/**\n * –¢–µ—Å—Ç getClientCredentials\n */\nfunction testGetClientCredentials() {\n  try {\n    var credentials = getClientCredentials();\n    \n    if (!credentials || typeof credentials !== 'object') {\n      return { success: false, error: 'Function returned invalid object' };\n    }\n    \n    if (!credentials.hasOwnProperty('ok')) {\n      return { success: false, error: 'Missing \"ok\" property' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç validateLicenseForGM\n */\nfunction testValidateLicenseForGM() {\n  try {\n    var result = validateLicenseForGM();\n    \n    if (!result || typeof result !== 'object') {\n      return { success: false, error: 'Function returned invalid object' };\n    }\n    \n    if (!result.hasOwnProperty('ok')) {\n      return { success: false, error: 'Missing \"ok\" property' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç SERVER_API_URL\n */\nfunction testServerApiUrl() {\n  try {\n    if (typeof SERVER_API_URL === 'undefined') {\n      return { success: false, error: 'SERVER_API_URL is undefined' };\n    }\n    \n    if (SERVER_API_URL.includes('YOUR_SERVER_ID')) {\n      return { success: false, error: 'SERVER_API_URL contains placeholder' };\n    }\n    \n    if (!SERVER_API_URL.startsWith('https://script.google.com/macros/s/')) {\n      return { success: false, error: 'SERVER_API_URL format is incorrect' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç addSystemLog\n */\nfunct