ess: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç getSystemStatusData\n */\nfunction testGetSystemStatusData() {\n  try {\n    if (typeof getSystemStatusData !== 'function') {\n      return { success: false, error: 'getSystemStatusData function does not exist' };\n    }\n    \n    var statusData = getSystemStatusData();\n    \n    if (!statusData || typeof statusData !== 'object') {\n      return { success: false, error: 'Function returned invalid object' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç importSocialPosts\n */\nfunction testImportSocialPosts() {\n  try {\n    if (typeof importSocialPosts !== 'function') {\n      return { success: false, error: 'importSocialPosts function does not exist' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç testGeminiConnection function\n */\nfunction testGeminiConnectionFunction() {\n  try {\n    if (typeof testGeminiConnection !== 'function') {\n      return { success: false, error: 'testGeminiConnection function does not exist' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç Client-Server communication\n */\nfunction testClientServerComm() {\n  try {\n    // –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ ThinClient —Å—É—â–µ—Å—Ç–≤—É–µ—Ç\n    if (typeof callServer !== 'function') {\n      return { success: false, error: 'callServer function does not exist' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç Sheet operations\n */\nfunction testSheetOperations() {\n  try {\n    var ss = SpreadsheetApp.getActive();\n    \n    if (!ss) {\n      return { success: false, error: 'Cannot access active spreadsheet' };\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç Properties management\n */\nfunction testPropertiesManagement() {\n  try {\n    var props = PropertiesService.getScriptProperties();\n    \n    if (!props) {\n      return { success: false, error: 'Cannot access script properties' };\n    }\n    \n    // –¢–µ—Å—Ç –∑–∞–ø–∏—Å–∏/—á—Ç–µ–Ω–∏—è\n    props.setProperty('TEST_KEY', 'test_value');\n    var value = props.getProperty('TEST_KEY');\n    \n    if (value !== 'test_value') {\n      return { success: false, error: 'Properties read/write failed' };\n    }\n    \n    // –û—á–∏—Å—Ç–∫–∞\n    props.deleteProperty('TEST_KEY');\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –¢–µ—Å—Ç Error handling\n */\nfunction testErrorHandling() {\n  try {\n    // –¢–µ—Å—Ç–∏—Ä—É–µ–º —á—Ç–æ —Å–∏—Å—Ç–µ–º–∞ –ø—Ä–∞–≤–∏–ª—å–Ω–æ –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ—Ç –æ—à–∏–±–∫–∏\n    try {\n      throw new Error('Test error');\n    } catch (e) {\n      // –û—à–∏–±–∫–∞ –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –ø–æ–π–º–∞–Ω–∞\n      if (e.message !== 'Test error') {\n        return { success: false, error: 'Error handling failed' };\n      }\n    }\n    \n    return { success: true };\n    \n  } catch (error) {\n    return { success: false, error: error.message };\n  }\n}\n\n/**\n * –ë—ã—Å—Ç—Ä—ã–π —Ç–µ—Å—Ç –∫—Ä–∏—Ç–∏—á–µ—Å–∫–∏—Ö —Ñ—É–Ω–∫—Ü–∏–π\n */\nfunction quickTest() {\n  var ui = SpreadsheetApp.getUi();\n  \n  var results = {\n    total: 0,\n    passed: 0,\n    failed: 0,\n    errors: []\n  };\n  \n  // –¢–µ—Å—Ç–∏—Ä—É–µ–º —Ç–æ–ª—å–∫–æ –∫—Ä–∏—Ç–∏—á–µ—Å–∫–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏\n  addTestResult(results, 'üîê getClientCredentials', testGetClientCredentials);\n  addTestResult(results, '‚öôÔ∏è SERVER_API_URL', testServerApiUrl);\n  addTestResult(results, 'üß† GM function', testGMFunctio